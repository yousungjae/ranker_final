	<div class="wrapper bg-dark-white">
			<!-- HEADING-BANNER START -->
			<div class="heading-banner-area overlay-bg">
				<div class="container">
					<div class="row">
						<div class="col-md-12">
							<div class="heading-banner">
								<div class="heading-banner-title">
									<h2>Registration</h2>
								</div>
								<div class="breadcumbs pb-15">
									<ul>
										<li><a href="/">Home</a></li>
										<li>Registration</li>
									</ul>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
			<!-- HEADING-BANNER END -->
			<!-- SHOPPING-CART-AREA START -->
			<div class="login-area  pt-80 pb-80">
				<div class="container">
					<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
				  	<div class="row">
  						<div class="col-md-6 col-sm-6 col-xs-12">
  							<div class="user-login text-left">
  								<h4 class="title-1 title-border text-uppercase mb-30">REGISTERED USER</h4>
  								<p class="text-gray">If you have an account with us, Please login!</p>
  								<%= f.text_field :email, autofocus: true, autocomplete: "email", placeholder: 'Email address here...' %>
			            <%= f.password_field :password, autocomplete: "off", placeholder: 'Password' %>
                  <%= f.submit "Log in", class: "button-one submit-button mt-15" %><br><br>
                  <%= render "devise/shared/links" %>
                </div>
              </div>
            </div>
          <% end %>
				</div>
			</div>
		</div>




